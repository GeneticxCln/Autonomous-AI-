{
  "dashboard": {
    "id": null,
    "title": "Worker Metrics & Queue Depth",
    "tags": ["agent", "workers", "queues"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 0,
    "time": {"from": "now-1h", "to": "now"},
    "panels": [
      {
        "id": 1,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "count(agent_worker_heartbeat{service=\"agent-worker\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Queue Depth (High Priority)",
        "type": "graph",
        "targets": [
          {
            "expr": "rq_queue_length{queue=\"high\"}",
            "refId": "A",
            "legendFormat": "High Priority"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Queue Depth (Normal Priority)",
        "type": "graph",
        "targets": [
          {
            "expr": "rq_queue_length{queue=\"normal\"}",
            "refId": "A",
            "legendFormat": "Normal Priority"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 20},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 0}
      },
      {
        "id": 4,
        "title": "Job Throughput (Jobs/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rq_jobs_total{status=\"succeeded\"}[5m])",
            "refId": "A",
            "legendFormat": "Completed"
          },
          {
            "expr": "rate(rq_jobs_total{status=\"failed\"}[5m])",
            "refId": "B",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Job Duration (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rq_job_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P95 Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Worker CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"agent-worker\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Worker Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"agent-worker\"} / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Memory (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "mbytes",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 512},
                {"color": "red", "value": 1024}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Queue Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rq_queue_length[5m])",
            "refId": "A",
            "legendFormat": "{{queue}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Failed Jobs Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rq_jobs_total{status=\"failed\"}[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "ops",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1.0}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Job Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rq_jobs_total{status=\"succeeded\"}[5m]) / (rate(rq_jobs_total{status=\"succeeded\"}[5m]) + rate(rq_jobs_total{status=\"failed\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 32}
      }
    ]
  }
}

