{
  "dashboard": {
    "id": null,
    "title": "Agent System Overview",
    "tags": ["agent", "system", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "time": {"from": "now-1h", "to": "now"},
    "panels": [
      {
        "id": 1,
        "title": "System Status",
        "type": "stat",
        "targets": [{"expr": "up{job=\"agent-api\"}", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {"steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]}
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Redis Status",
        "type": "stat",
        "targets": [{"expr": "up{job=\"redis\"}", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {"steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]}
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "API Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"agent-api\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1.0}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Active Tasks",
        "type": "stat",
        "targets": [{"expr": "rq_jobs_total{status=\"started\"}", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 8,
        "title": "System Health Score",
        "type": "stat",
        "targets": [{"expr": "agent_system_health_score", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 9,
        "title": "Active Incidents",
        "type": "stat",
        "targets": [{"expr": "agent_active_incidents_total", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 10,
        "title": "AI Decision Accuracy",
        "type": "stat",
        "targets": [{"expr": "agent_decision_accuracy_ratio", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.75},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8}
      },
      {
        "id": 11,
        "title": "Goal Completion Rate",
        "type": "stat",
        "targets": [{"expr": "agent_goal_completion_rate", "refId": "A"}],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.85}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8}
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "graph",
        "targets": [{"expr": "rate(http_requests_total{job=\"agent-api\"}[5m])", "refId": "A"}],
        "yAxes": [{"label": "Requests/sec", "min": 0}],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [{"expr": "avg_over_time(cache_hit_ratio[5m])", "refId": "A"}],
        "yAxes": [{"label": "Hit Rate", "min": 0, "max": 1}],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 12,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {"expr": "system_cpu_usage_percent", "refId": "A", "legendFormat": "CPU %"},
          {"expr": "system_memory_usage_percent", "refId": "B", "legendFormat": "Memory %"},
          {"expr": "system_disk_usage_percent", "refId": "C", "legendFormat": "Disk %"}
        ],
        "yAxes": [{"label": "Percent", "min": 0, "max": 100}],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Task Queue Status",
        "type": "table",
        "targets": [{"expr": "rq_jobs_total", "refId": "A"}],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 13,
        "title": "Decision Latency (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_decision_latency_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "yAxes": [{"label": "Seconds", "min": 0}],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 14,
        "title": "Alert Volume",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(increase(agent_alert_events_total[1h]))",
            "refId": "A"
          }
        ],
        "yAxes": [{"label": "Alerts/hour", "min": 0}],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ]
  }
}
