{
  "dashboard": {
    "id": null,
    "title": "Job Throughput & Performance",
    "tags": ["agent", "jobs", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "time": {"from": "now-1h", "to": "now"},
    "panels": [
      {
        "id": 1,
        "title": "Total Jobs Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(rq_jobs_total[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "blue", "value": null}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Jobs Per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rq_jobs_total{status=\"succeeded\"}[1m])",
            "refId": "A",
            "legendFormat": "Succeeded"
          },
          {
            "expr": "rate(rq_jobs_total{status=\"failed\"}[1m])",
            "refId": "B",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Job Duration Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(rq_job_duration_seconds_bucket[5m])",
            "refId": "A",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "spectrum"}
          }
        },
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Job Duration Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(rq_job_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(rq_job_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(rq_job_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        },
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Jobs by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (job_type) (increase(rq_jobs_total[1h]))",
            "refId": "A",
            "legendFormat": "{{job_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 10, "w": 8, "x": 0, "y": 18}
      },
      {
        "id": 6,
        "title": "Jobs by Priority",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum by (priority) (rq_queue_length)",
            "refId": "A",
            "legendFormat": "{{priority}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 10, "w": 8, "x": 8, "y": 18}
      },
      {
        "id": 7,
        "title": "Job Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rq_jobs_total{status=\"failed\"}[5m]) / rate(rq_jobs_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 18}
      },
      {
        "id": 8,
        "title": "Average Queue Wait Time",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(rq_job_wait_time_seconds)",
            "refId": "A",
            "legendFormat": "Wait Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 30}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 9,
        "title": "Worker Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "rq_workers_busy / rq_workers_total * 100",
            "refId": "A",
            "legendFormat": "Utilization %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      }
    ]
  }
}

